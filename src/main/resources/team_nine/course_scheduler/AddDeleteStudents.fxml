               </VBox.margin>
            </ListView>
            <HBox alignment="BASELINE_RIGHT" prefHeight="42.0" prefWidth="599.0">
               <children>
                  <Button fx:id="addStudentButton" mnemonicParsing="false" prefWidth="52.5" text="Add">
                     <HBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="deleteStudentButton" mnemonicParsing="false" onAction="#deleteStudentsFromCourse" prefWidth="52.5" text="Delete">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
